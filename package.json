{
  "name": "tennis-string-advisor",
  "version": "2.0.0",
  "description": "Tennis String Advisor - Modernized platform for racquet and string recommendations",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "audit:security": "npm audit && npm run audit:dependencies",
    "audit:dependencies": "node scripts/audit-dependencies.js",
    "audit:performance": "node scripts/audit-performance.js",
    "audit:seo": "node scripts/audit-seo.js",
    "audit:accessibility": "node scripts/audit-accessibility.js",
    "audit:all": "npm run audit:security && npm run audit:dependencies && npm run audit:performance && npm run audit:seo && npm run audit:accessibility",
    "scrape:tennis-warehouse": "node scripts/scraper/tennis-warehouse.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node scripts/seed-database.js",
    "test": "vitest",
    "test:e2e": "playwright test",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@prisma/client": "^5.17.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "next-auth": "^4.24.7",
    "@stripe/stripe-js": "^4.1.0",
    "stripe": "^16.5.0",
    "zod": "^3.23.8",
    "react-hook-form": "^7.52.1",
    "@hookform/resolvers": "^3.9.0",
    "zustand": "^4.5.4",
    "@tanstack/react-query": "^5.51.15",
    "framer-motion": "^11.3.8",
    "lucide-react": "^0.417.0",
    "tailwindcss": "^3.4.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.4.0",
    "date-fns": "^3.6.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "@types/node": "^20.14.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "cross-env": "^7.0.3",
    "prisma": "^5.17.0",
    "vitest": "^2.0.5",
    "jsdom": "^24.1.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.4.8",
    "playwright": "^1.46.0",
    "lighthouse": "^12.1.0",
    "puppeteer": "^23.0.2",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.7.3",
    "dotenv": "^16.4.5",
    "@next/bundle-analyzer": "^14.2.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PleneufMC/Tennis-String-Advisor.git"
  },
  "author": "Tennis String Advisor Team",
  "license": "MIT"
}